{
  "name": "project-processor-es",
  "version": "1.0.0",
  "description": "Topcoder - Project ES Processor",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "sync:es": "node migrations/elasticsearch_sync.js",
    "view-data": "node test/common/view-data.js",
